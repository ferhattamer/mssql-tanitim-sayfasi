<div class="alert alert-secondary mt-4">
    <span class="oi oi-pencil me-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
        takip edebilmek için lütfen GITHUB hesabıma
        <a target="_blank" class="font-weight-bold link-dark" href="https://github.com/ferhattamer">linke tıklayarak</a>
    </span>
    ulaşabilirsiniz ve serüvenimize başlayabiliriz.
</div>

@code {
    // Demonstrates how a parent component can supply parameters
    [Parameter]
    public string? Title { get; set; }
}
